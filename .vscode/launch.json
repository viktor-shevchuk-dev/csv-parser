  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "address": "localhost",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run ts-node",
      "runtimeExecutable": "ts-node",
      "cwd": "${workspaceFolder}/server",
      "args": []
    }
  ]
}
