{
  "name": "csv-parser-server",
  "version": "0.1.0",
  "description": "CSV API Server",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src -e ts --exec 'node --inspect=9229 -r ts-node/register src/server.ts'",
    "build": "tsc",
    "lint": "eslint ./src",
    "test": "jest",
    "cluster": "pm2 start dist/server.js -i max",
    "delete": "pm2 delete all"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@types/cors": "^2.8.17",
    "@types/csv-write-stream": "^2.0.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/stream-json": "^1.7.8",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "p-queue": "^8.1.0",
    "pm2": "^5.4.3",
    "stream-json": "^1.9.1",
    "typescript": "^5.7.3"
  }
}
